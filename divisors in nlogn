#include<iostream>
#include<vector>

using namespace std;

int main(){

    int N = 1e6 + 10;
    vector<vector<int>> ans(N);
    for(int i = 1;i<N;i++){
        for(int j = i;j<N;j+=i){
            ans[j].push_back(i);
        }
    }


  
    int t;
    cin>>t;
    while(t--){
        int x;cin >> x;
        cout << ans[x].size() << endl;
    }
}
